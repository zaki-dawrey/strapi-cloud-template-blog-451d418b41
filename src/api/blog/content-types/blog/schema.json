{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "blog"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "meta_title": {
      "type": "string"
    },
    "meta_description": {
      "type": "text"
    },
    "focus_keyword": {
      "type": "string"
    },
    "secondary_keywords": {
      "type": "string"
    },
    "featured_image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "published_date": {
      "type": "date"
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "blog.rich-text",
        "blog.image-block",
        "blog.table",
        "blog.faq",
        "blog.cta"
      ]
    }
  }
}
