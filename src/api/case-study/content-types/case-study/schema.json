      "type": "string"
    },
    "meta_description": {
      "type": "text"
    },
    "client_name": {
      "type": "string"
    },
    "client_location": {
      "type": "string"
    },
    "industry": {
      "type": "string"
    },
    "featured_image": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
